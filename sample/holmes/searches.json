      "inside": [
        {
          "hf_id": "hf_hf4",
          "score": 0.6310368075765799,
          "distance": 0.2677548922578743,
          "text": "Mr. Rucastle told funny stories to Miss Hunter in the drawing-room.",
          "classes": [
            "Mr. Rucastle",
            "Miss Hunter",
            "drawing-room"
          ],
          "sources": [
            "ch205"
          ]
        },
        {
          "hf_id": "hf_hf8",
          "score": 0.5895235995982689,
          "distance": 0.35702794687975226,
          "text": "Mr. Rucastle warned Miss Hunter not to go near the mastiff at night.",
          "classes": [
            "Mr. Rucastle",
            "Miss Hunter",
            "mastiff"
          ],
          "sources": [
            "ch207"
          ]
        },
        {
          "hf_id": "hf_hf28",
          "score": 0.5824907573886031,
          "distance": 0.37341235007148416,
          "text": "Dr. Grimesby Roylott confronted Holmes and Watson, accusing them of meddling in his affairs.",
          "classes": [
            "Dr. Roylott",
            "Holmes",
            "Dr. Watson"
          ],
          "sources": [
            "ch131"
          ]
        },
        {
          "hf_id": "hf_hf17",
          "score": 0.5808988122446329,
          "distance": 0.3771761675820011,
          "text": "Miss Hunter planned to send a wire to Holmes for help.",
          "classes": [
            "Miss Hunter",
            "Holmes"
          ],
          "sources": [
            "ch210"
          ]
        },
        {
          "hf_id": "hf_hf18",
          "score": 0.5808988122446329,
          "distance": 0.3771761675820011,
          "text": "Holmes deduced that Miss Hunter was brought to impersonate someone.",
          "classes": [
            "Holmes",
            "Miss Hunter"
          ],
          "sources": [
            "ch211"
          ]
        },
        {
          "hf_id": "hf_hf26",
          "score": 0.5753077494444739,
          "distance": 0.390560097221865,
          "text": "Holmes noted that the flooring and walls of the room where Julia died were sound and secure.",
          "classes": [
            "Holmes",
            "Julia"
          ],
          "sources": [
            "ch130"
          ]
        },
        {
          "hf_id": "hf_hf3",
          "score": 0.5701237183555208,
          "distance": 0.4032042068124095,
          "text": "Miss Hunter wore an electric-blue dress provided by Mr. Rucastle.",
          "classes": [
            "Miss Hunter",
            "Mr. Rucastle",
            "electric-blue dress"
          ],
          "sources": [
            "ch205"
          ]
        },
        {
          "hf_id": "hf_hf7",
          "score": 0.5654863664446961,
          "distance": 0.41471138381236133,
          "text": "Mrs. Rucastle identified the man in the road as impertinent.",
          "classes": [
            "Mrs. Rucastle",
            "Miss Hunter"
          ],
          "sources": [
            "ch206"
          ]
        },
        {
          "hf_id": "hf_hf1",
          "score": 0.5557327298594624,
          "distance": 0.4395409106141896,
          "text": "Miss Hunter cut her hair at the request of Mr. Rucastle.",
          "classes": [
            "Miss Hunter",
            "Mr. Rucastle"
          ],
          "sources": [
            "ch205"
          ]
        },
        {
          "hf_id": "hf_hf2",
          "score": 0.5557327298594624,
          "distance": 0.4395409106141896,
          "text": "Mr. Rucastle complimented Miss Hunter on her appearance after cutting her hair.",
          "classes": [
            "Mr. Rucastle",
            "Miss Hunter"
          ],
          "sources": [
            "ch205"
          ]
        },
        {
          "hf_id": "hf_hf16",
          "score": 0.5557327298594624,
          "distance": 0.4395409106141896,
          "text": "Mr. Rucastle threatened Miss Hunter after she entered the locked room.",
          "classes": [
            "Mr. Rucastle",
            "Miss Hunter"
          ],
          "sources": [
            "ch210"
          ]
        },
        {
          "hf_id": "hf_hf27",
          "score": 0.5516539510278714,
          "distance": 0.4501844834237058,
          "text": "Holmes planned to visit Stoke Moran to investigate the case further.",
          "classes": [
            "Holmes",
            "Stoke Moran"
          ],
          "sources": [
            "ch130"
          ]
        }
      ],
      "periphery": [
